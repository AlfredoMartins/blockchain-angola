{
  "expo": {
    "name": "Blockchain Angola",
    "slug": "election-blockchain-angola",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonEmptEncryption": false
      },
      "usesIcloudStorage": true,
      "bundleIdentifier": "com.blockchain.election",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./assets/favicon.png"
      },
      "plugins": [
        "expo-font",
        "expo-secure-store",
        "expo-av"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "23db0c99-ac06-4556-8c80-693fb773a032"
      }
    },
    "owner": "alfredomartins624",
    "plugins": [
      "expo-font"
    ],
    "android": {
      "package": "com.blockchain.election"
    }
  }
}
